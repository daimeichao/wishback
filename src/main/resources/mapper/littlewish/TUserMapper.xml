<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiading.modules.littlewish.mapper.TUserMapper">
    <update id="editinfo">
        UPDATE t_user
        SET `name` = #{name}
        WHERE pid = #{pid}
    </update>

    <select id="getUserById" resultType="com.jiading.modules.littlewish.domain.WishUser">
        select *
        FROM t_user
        WHERE openid = #{openid}
    </select>
</mapper>
